                    <a href="shop.html">Shop all collection</a>
                </div>
            </div>
        </div> 
        <div class="row">
           <!-- <div class="product_carousel product_column5 owl-carousel"> -->
            <%if(locals.productAvailable){%>
                <%productAvailable.forEach((el)=>{%>
                    <div class="col-lg-3">
                        <div class="product_items" >
                             <article class="single_product" >
                                 <figure>
                                     <div class="product_thumb" style="aspect-ratio: 1/1;">
                                         <a class="primary_img" href="/usershop"><img src="/assets/<%= el.img[0]%>" alt="" ></a>
